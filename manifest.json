{
    "manifest_version": 2,
    "name": "RSS Scraper",
    "version": "0.1",
    "description": "Scrapes pages for RSS Feeds, posts found feed URLs to feeds.128.io",
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],
    "permissions": [
        "webNavigation"
    ]    
}
